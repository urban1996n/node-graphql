{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "bundler",
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "declaration": true,
        "emitDeclarationOnly": false,
        "outDir": "dist",
        "baseUrl": "."
    },
    "include": ["src"]
}
